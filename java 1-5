1)
import java.util.Scanner;
class bank
{
    public int deposit, withdraw,balance;
    public String name;

    public bank()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter deposit money");
        deposit=sc.nextInt();
        balance=deposit;
    }
}
class savingsbank extends bank
{
    public savingsbank()
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("enter name ");
        name=sc.nextLine();
        System.out.print("enetr withdrawal money");
        withdraw=sc.nextInt();


    }
}
public class main
{
public static void main(String args[])
{
    bank bm =new bank();
    savingsbank sa=new savingsbank();
    System.out.println("account holder name :"+sa.name);
    System.out.println("deposited money :"+bm.deposit);
    System.out.print("total money after withdrawl");
    bm.balance=bm.balance-sa.withdraw;
    System.out.print(bm.balance);

}
}





